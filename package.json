{
  "name": "express-es6-rest-api",
  "version": "0.3.0",
  "engines": {
    "node": "12.13.0"
  },
  "description": "Starter project for an ES6 RESTful Express API",
  "main": "server.js",
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": "developit/express-es6-rest-api",
  "author": "Jason Miller <jason@developit.ca>",
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/node": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "babel-jest": "^24.9.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jest": "^24.9.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.7.12",
    "morgan": "^1.8.0",
    "multer": "^1.4.2",
    "resource-router-middleware": "^0.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "eslint": "^6.7.1",
    "eslint-plugin-jest": "^23.0.5",
    "jest-junit": "^9.0.0",
    "nodemon": "^1.9.2"
  }
}
